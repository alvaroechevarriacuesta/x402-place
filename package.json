{
  "name": "x402place",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.3",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build --filter=@x402place/app",
    "app:dev": "pnpm --filter @x402place/app run dev",
    "backend:dev": "pnpm --filter @x402place/backend run dev",
    "worker:start": "pnpm --filter @x402place/worker run start",
    "seed": "tsx scripts/seed-db.ts",
    "format": "prettier --write .",
    "test:pixels": "tsx scripts/test-place-pixels.ts"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "tsx": "^4.20.6",
    "turbo": "^2.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3"
  }
}
